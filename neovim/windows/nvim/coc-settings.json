  "coc.preferences.formatOnType": true,
  "coc.preferences.formatOnSave": true,
  // coc-prettier
  "prettier.disableLanguages": ["tex"],
  // coc-sumneko-lua config
  "Lua.misc.executablePath": "lua-language-server",
